{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
        "files": ["static/**/*.js"],
        "rules": {
            "no-undef": "off"
        }
    }
  ]
}
